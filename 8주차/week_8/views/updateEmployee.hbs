<h1>{{title}}</h1>  <!--Employee-->
<table>
    <tr> <!--Employee 속성-->
        <td>Fname</td>
        <td>Minit</td>
        <td>Fname</td>
        <td>Ssn</td>
        <td>Bdate</td>
        <td>Address</td>
        <td>Sex</td>
        <td>Salary</td>
        <td>Super_ssn</td>
        <td>Dno</td>
    </tr>
    {{#each emp_res}} <!--employee emp_res의 반복하여 출력-->
        <form method="post" action='/update/employee'>
            <tr>
                <td>{{Fname}}</td>
                <td>{{Minit}}</td>
                <td>{{Fname}}</td>
                <td>{{Ssn}}</td>
                <td>{{Bdate}}</td>
                <td>{{Address}}</td>
                <td>{{Sex}}</td>
                <td><input type="text" name="salary" placeholder={{Salary}}></td>
                <td>{{Super_ssn}}</td>
                <td>{{Dno}}</td>
                <td><input type="submit" value="수정"></td>
            </tr>
        </form>
    {{/each}}
</table>